	"setup global theme": {
		"prefix": "createGlobalStyle",
		"body": [
			"createGlobalStyle`",
			"  html {",
			"    box-sizing: border-box;",
			"    font-size: 10px;",
			"  }",
			"  *, *:before, *:after {",
			"    box-sizing: inherit;",
			"  }",
			"  body {",
			"    padding: 0;",
			"    margin: 0;",
			"    font-size: $1;",
			"  }",
			"`;"
		],
		"description": "setup global theme"
